<div class="container text-xs-center">
  <div class="row">
    <div class="col-md-3" style="background-color:gainsboro">
      <div class="row">

        <h3>Dashboard</h3>
      </div>
      <div class="row">
        <div class="col-md-12">
          <%= image_tag current_user.image_url , class:"img-responsive", style:"width:100%"%>
        </div>
      </div>
      <%#= binding.pry %>
      <br>
      <div class="row">
        <h3><%= current_user.name %> </h3>
      </div>
      <div class="row">
        <div class="col-md-4">
          Starred:<%= @starred%>
        </div>
        <div class="col-md-4">
          Followers:<%= @user["followers"]%>
        </div>
        <div class="col-md-4">
          Following:<%= @user["following"]%>
        </div>
      </div>
      <%= link_to "SuperTest", "/supertest" %>
      <%= link_to "Logout", "/logout" %>

    </div>
  </div>
</div>
